<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="monster.fr.wcs.monsterwikilegend">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icone"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CreateOrViewList" />
        <activity android:name=".CreateScreen" />
        <activity android:name=".ListScreen" />
        <activity android:name=".CreateScreenFeatures" />
        <activity android:name=".ViewMonster"></activity>
    </application>

</manifest>